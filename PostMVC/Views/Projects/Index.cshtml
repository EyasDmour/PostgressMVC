@model IEnumerable<PostMVC.Models.Projects>

<h1 class="display-4">Projects</h1>
<table class="table table-striped table-hover mt-4">
    <thead class="thead-light">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Description</th>
            <th>Start Date</th>
            <th>End Date</th>
            <th>Priority</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.Description
            </td>
            <td>
                @item.StartDate
            </td>
            <td>
                @item.EndDate
            </td>
            <td>
                @item.Priority
            </td>
        </tr>
}
<tr>
    <td></td>
    <td></td>
    <td></td>
    <td>
        <a asp-action="Create" class="btn btn-sm btn-primary">Create</a>
    </td>
    <td></td>
    <td></td>
</tr>

    </tbody>
</table>